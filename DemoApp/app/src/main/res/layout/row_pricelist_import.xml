<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/table_price_background"
    android:layout_marginStart="10dp"
    android:layout_height="wrap_content">

    <TableRow>
        <TextView
            android:layout_width="100dp"
            android:id="@+id/tv_row_price_import_stt"
            android:layout_marginStart="2dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:layout_column="1"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_pol"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_pod"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_of20"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold">
        </TextView>

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_of40"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="250dp"
            android:id="@+id/tv_row_price_import_surcharge"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_total"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_carrier"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_schedule"
            android:layout_marginStart="1dp"
            android:textColor="@color/black"
            android:gravity="center"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_transit"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_free"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_valid"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_import_note"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />
    </TableRow>

</TableLayout>